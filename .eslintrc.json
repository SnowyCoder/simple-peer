{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-empty": "off",
        "lines-between-class-members": "off",
        "no-shadow": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "prefer-destructuring": "off",
        "no-undef": "off",
        "no-use-before-define": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error"],
        "max-len": "off"
    }
}
